{
  "name": "@scalableminds/saxophone",
  "description": "Fast and lightweight event-driven XML parser in pure JavaScript",
  "version": "0.3.0",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/matteodelabre/saxophone.git"
  },
  "scripts": {
    "build": "webpack -p --progress --colors",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "tape": "tape 'lib/**/*.test.js'",
    "test": "npm run --silent tape | faucet",
    "coverage": "nyc --reporter=html --reporter=text npm --silent run tape",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "benchmark": "node benchmark"
  },
  "nyc": {
    "exclude": [
      "dist",
      "node_modules",
      "lib/**/*.test.js"
    ]
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "4 || 6 || 8 || 9"
  },
  "keywords": [
    "xml",
    "parser",
    "stream",
    "sax",
    "fast"
  ],
  "author": {
    "name": "Mattéo Delabre ✏️",
    "email": "contact@matteodelabre.me",
    "url": "https://github.com/matteodelabre"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "benchmark": "^2.1.4",
    "common-tags": "^1.6.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.14.0",
    "faucet": "0.0.1",
    "lodash.uniq": "^4.5.0",
    "nyc": "^11.4.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "readable-stream": "^2.3.3",
    "string_decoder": "^1.0.3"
  }
}
