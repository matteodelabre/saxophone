{
  "name": "saxophone",
  "description": "Fast and lightweight event-driven XML parser in pure JavaScript",
  "version": "0.5.0",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "./types/saxophone.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/matteodelabre/saxophone.git"
  },
  "scripts": {
    "lint": "eslint .",
    "tape": "tape 'lib/**/*.test.js'",
    "test": "npm run --silent tape | faucet",
    "coverage": "nyc --reporter=html --reporter=text npm --silent run tape",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "benchmark": "node benchmark"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "lib/**/*.test.js"
    ]
  },
  "engines": {
    "node": "6 || 8 || 10"
  },
  "keywords": [
    "xml",
    "parser",
    "stream",
    "sax",
    "fast"
  ],
  "author": {
    "name": "Matt√©o Delabre",
    "email": "contact@matteodelabre.me",
    "url": "https://github.com/matteodelabre"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "common-tags": "^1.6.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.14.0",
    "faucet": "0.0.1",
    "lodash.uniq": "^4.5.0",
    "nyc": "^13.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.8.0",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "readable-stream": "^3.0.3",
    "string_decoder": "^1.1.1"
  }
}
